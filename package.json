{
  "name": "@remotelinker/reverse-ws-tunnel",
  "version": "1.0.9",
  "description": "A Node.js library for creating secure reverse tunnels over WebSocket connections",
  "main": "index.cjs",
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remoteLinker/reverse-ws-tunnel.git"
  },
  "homepage": "https://github.com/remoteLinker/reverse-ws-tunnel#readme",
  "bugs": {
    "url": "https://github.com/remoteLinker/reverse-ws-tunnel/issues"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.cjs",
    "client/",
    "server/",
    "utils/",
    "types/"
  ],
  "config": {
    "dockerRegistry": {
      "prod": "",
      "localDev": ""
    }
  },
  "scripts": {
    "test": "jest",
    "test:safe": "node run-tests.js",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit examples/typescript/*.ts",
    "build:types": "tsc",
    "pack": "npm pack && mv *.tgz dist/",
    "docker:build": "cross-conf-env docker image build -f Dockerfile . -t $npm_package_config_dockerRegistry_prodremotelinker/reverse-ws-tunnel:$npm_package_version",
    "docker:deploy": "docker-compose up -d",
    "example:server": "node examples/server/server-example.js",
    "example:server:esm": "node examples/server/server-example.mjs",
    "example:client": "node examples/client/client-example.js",
    "example:client:esm": "node examples/client/client-example.mjs",
    "example:webserver": "node examples/webserver/webserver-example.js",
    "format": "prettier --write \"**/*.js\" \"**/*.mjs\" \"**/*.cjs\"",
    "format:check": "prettier --check \"**/*.js\" \"**/*.mjs\" \"**/*.cjs\""
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./server": {
      "import": "./server/index.mjs",
      "require": "./server/index.js"
    },
    "./client": {
      "import": "./client/index.mjs",
      "require": "./client/index.js"
    },
    "./utils": {
      "import": "./utils/index.mjs",
      "require": "./utils/index.js"
    }
  },
  "keywords": [],
  "author": "Andrea Trentin <at30in@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "cookie": "^1.0.2",
    "cross-conf-env": "^1.3.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "http-parser-js": "^0.5.10",
    "http-proxy": "^1.18.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  }
}
